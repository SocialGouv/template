{
  "name": "template",
  "version": "1.14.6",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "prebuild": "node -r @swc-node/register scripts/prebuild.ts",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "e2e": "cypress open",
    "e2e:headless": "cypress run",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "start-server-and-test dev http://localhost:3000 e2e",
    "test:e2e:headless": "start-server-and-test dev http://localhost:3000 e2e:headless",
    "storybook:start": "start-storybook -p 6006",
    "storybook:build": "build-storybook -c .storybook -o .out",
    "storybook:deploy": "npm run storybook:build && gh-pages -d .out",
    "storybook:deploy:action": "npm run storybook:build && gh-pages -d .out -u \"github-actions-bot <support+actions@github.com>\"",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "keycloak": "chmod +x keycloak/scripts/keycloak.sh && keycloak/scripts/keycloak.sh"
  },
  "dependencies": {
    "@dataesr/react-dsfr": "^3.0.0",
    "@gouvfr/dsfr": "^1.7.0",
    "@sentry/nextjs": "^7.8.1",
    "@socialgouv/matomo-next": "^1.2.2",
    "next": "^12.2.3",
    "next-auth": "4.10.3",
    "next-seo": "^5.1.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "remixicon": "^2.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-interactions": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/react": "^6.4.19",
    "@storybook/testing-library": "^0.0.13",
    "@swc-node/register": "^1.4.2",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.2",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.9",
    "cypress": "^9.4.1",
    "eslint": "8.24.0",
    "eslint-config-next": "12.3.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-storybook": "^0.6.0",
    "gh-pages": "^4.0.0",
    "hasura-cli": "^2.9.0",
    "husky": "^8.0.1",
    "is-ci": "^3.0.1",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.7",
    "node-talisman": "^1.28.0",
    "start-server-and-test": "^1.14.0",
    "tsc-files": "^1.1.3",
    "typescript": "4.8.4"
  }
}
